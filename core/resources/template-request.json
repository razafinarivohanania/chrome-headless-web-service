{
    "request": {
        "url": "",
        "method": "",
        "parameters": "",
        "headers": {
            "key": "value"
        }
    },
    "proxy": {
        "host": "",
        "port": ""
    },
    "skip": {
        "resources": [
            "image",
            "audio",
            "css",
            "video",
            "javascript"
        ],
        "url": {
            "equals": [
                "http://www.site.com/0",
                "http://www.site.com/1"
            ],
            "equalsIgnoreCase" : [
                "http://www.site.com/0",
                "http://www.site.com/1"
            ],
            "contains": [
                "www.google.com",
                "www.facebook.com"
            ],
            "containsIgnoreCase" : [
                "www.google.com",
                "www.facebook.com"
            ],
            "patterns": [
                "[^\\.]+\\.google.[^\\.]+",
                "[^\\.]+\\.facebook.[^\\.]+"
            ],
            "function" : "function (url){return true}"
        }
    },
    "script" : "",
    "interceptor" : {
        "request" : "request => {this.continue()}",
        "response" : "(response, out) => {out.write('hello world!')}"
    },
    "activeFileLog" : true,
    "show" : [
        "request.headers",
        "response.headers",
        "response.body.intercepted"
    ],
    "hide" : [
        
    ]
}